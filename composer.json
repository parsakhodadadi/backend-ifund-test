{
  "autoload": {
    "files": [
      "Core/System/functions.php",
      "Routes.php"
    ],
    "classmap": [
      "app/Middlewares",
      "Core/System/Helpers",
      "Core/System",
      "app/Requests",
      "app/Models",
      "app/Controllers",
      "Helpers",
      "app/exceptions"
    ]
  },
  "psr-4": {
    "App\\Models\\": "app/Models",
    "App\\Controllers\\": "app/Controllers",
    "App\\Requests\\": "app/Requests",
    "App\\Middlewares\\": "app/Middlewares",
    "Helper\\": "Helpers"
  },
  "require": {
    "bramus/router": "~1.6",
    "requtize/query-builder": "dev-master",
    "jenssegers/blade": "^1.4",
    "robmorgan/phinx": "^0.13.4",
    "squizlabs/php_codesniffer": "3.*"
  },

  "config": {
    "platform": {
      "php": "7.1.0"
    }
  }

}
